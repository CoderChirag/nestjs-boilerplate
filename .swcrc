{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": "inline",
  "module": {
    "type": "commonjs",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
    "ignoreDynamic": false
  },
  "jsc": {
    "externalHelpers": false,
    "target": "es2020",
    "loose": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true,
      "importAssertions": false
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "baseUrl": "./src",
    "paths": {
      "src/*": [
        "./*"
      ]
    }
  },
  "minify": true
}